<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <DebugType>portable</DebugType>
    <AssemblyName>circuitbreaker_console_app</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>circuitbreaker_console_app</PackageId>
    <AssetTargetFallback>$(PackageTargetFallback);portable-net45+wp80+win8+wpa81+dnxcore50</AssetTargetFallback>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel" Version="1.1.0" />
    <PackageReference Include="System.Threading" Version="4.3.0" />
    <PackageReference Include="Polly" Version="5.0.2-v5-0-alpha0001" />
  </ItemGroup>

</Project>
